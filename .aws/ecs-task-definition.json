{
  "family": "graph",
  "executionRoleArn": "arn:aws:iam::445789524075:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "graph",
      "image": "445789524075.dkr.ecr.us-east-1.amazonaws.com/pokernook/graph:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
