{
  "name": "graph",
  "description": "GraphQL, Nexus Schema & Prisma",
  "repository": "github:pokernook/graph",
  "version": "0.0.0",
  "scripts": {
    "dev:prisma": "nodemon --watch \"prisma/schema.prisma\" --exec \"npm run prisma:db:push\"",
    "dev:server": "ts-node-dev --transpile-only api",
    "dev:typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "dev": "dotenv -- concurrently --kill-others npm:dev:*",
    "prisma:generate": "prisma generate",
    "prisma:migrate:save": "prisma migrate save --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "prisma:db:push": "prisma db push --preview-feature --ignore-migrations --force",
    "prisma:db:seed": "ts-node prisma/seeds/seed",
    "nexus:reflect": "ts-node --transpile-only api/server/schema",
    "generate": "npm run prisma:generate && npm run nexus:reflect",
    "build": "ncc build api/index.ts --quiet --out dist"
  },
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@prisma/client": "^2.11.0",
    "@vercel/ncc": "^0.25.1",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.24.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.11.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "concurrently": "^5.3.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-simple-import-sort": "^6.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.0"
  }
}
