{
  "name": "graph",
  "description": "GraphQL API built with Nexus and Prisma",
  "repository": "github:pokernook/graph",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint --fix --ext .ts .",
    "format": "prettier --write .",
    "dev": "nexus dev",
    "build": "nexus build",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "seed": "npx ts-node prisma/seeds/seed",
    "db:up": "docker-compose up -d && npm run migrate:up && npm run seed",
    "db:down": "docker-compose down"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "2.7.0",
    "apollo-server": "^2.18.1",
    "argon2": "^0.27.0",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-jwt-auth": "^1.4.0",
    "nexus-plugin-prisma": "^0.20.0",
    "serverless-http": "^2.6.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.7.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "esbuild": "^0.7.8",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.1.2",
    "serverless": "^2.2.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.8.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
