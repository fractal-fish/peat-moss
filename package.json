{
  "name": "graphql",
  "description": "GraphQL API built with Nexus and Prisma",
  "repository": "github:pokernook/graphql",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:deploy": "prisma migrate deploy --preview-feature",
    "prisma:db:push": "prisma db push --force --preview-feature",
    "prisma:db:seed": "prisma db seed --preview-feature",
    "nexus:reflect": "ts-node --transpile-only src/schema",
    "generate": "npm run prisma:generate && npm run nexus:reflect",
    "dev:prisma": "nodemon --watch prisma --ext \"*\" --exec \"npm run prisma:db:push && npm run prisma:db:seed\"",
    "dev:server": "ts-node-dev --transpile-only src",
    "dev:typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "dev": "dotenv -- concurrently --kill-others npm:dev:*",
    "lint": "eslint . --ext .js,.ts",
    "build": "ncc build src/index.ts --out dist"
  },
  "dependencies": {
    "@prisma/client": "2.15.0",
    "@vercel/ncc": "0.26.0",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "connect-redis": "^5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fastify": "^3.11.0",
    "fastify-cookie": "^5.1.0",
    "fastify-helmet": "^5.2.0",
    "fastify-session": "^5.2.1",
    "graphql": "^15.4.0",
    "helmet": "^4.3.1",
    "ioredis": "^4.19.4",
    "joi": "^17.3.0",
    "mercurius": "^6.11.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.29.0",
    "nexus-shield": "^2.0.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.15.0",
    "@types/connect-redis": "^0.0.16",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "concurrently": "^5.3.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.0",
    "typescript": "^4.1.0"
  }
}
