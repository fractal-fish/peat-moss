datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  // Specify targets for native machine and serverless environment
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model User {
  id           String @id @default(cuid())
  email        String @unique
  passwordHash String
}
