datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
}

model User {
  id            String @id @default(cuid())
  email         String @unique
  username      String
  discriminator Int
  passwordHash  String

  @@unique([username, discriminator], name: "UserTag")
}
