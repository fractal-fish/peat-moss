{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": ".build",
    "rootDir": ".",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020",
    "typeRoots": ["node_modules/@types", "generated/@types"]
  }
}
